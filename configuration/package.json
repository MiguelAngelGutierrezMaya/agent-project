{
  "name": "configuration",
  "version": "1.0.0",
  "description": "Service to manage the configuration of the application",
  "bin": {
    "cdk": "bin/cdk.js"
  },
  "scripts": {
    "dev": "tsx src/main.ts",
    "build": "tsc && tsc-alias",
    "watch": "tsc -w",
    "start": "node dist/main.js",
    "clean": "rm -rf dist",
    "test": "jest",
    "cdk": "cdk",
    "cdk:bootstrap": "cdk bootstrap",
    "cdk:synth": "cdk synth",
    "cdk:list:dev": "./scripts/list-stacks.sh development",
    "cdk:list:prod": "./scripts/list-stacks.sh production",
    "cdk:deploy:dev": "./scripts/deploy-stacks.sh development --bootstrap \"$@\"",
    "cdk:deploy:prod": "./scripts/deploy-stacks.sh production --bootstrap \"$@\"",
    "cdk:destroy:dev": "./scripts/destroy-stacks.sh development --yes \"$@\"",
    "cdk:destroy:prod": "./scripts/destroy-stacks.sh production --yes \"$@\""
  },
  "keywords": [
    "configuration"
  ],
  "author": "",
  "license": "ISC",
  "packageManager": "pnpm@10.15.1",
  "devDependencies": {
    "@types/aws-lambda": "^8.10.152",
    "@types/jest": "^29.5.14",
    "@types/node": "^24.5.1",
    "@types/pg": "^8.10.9",
    "aws-cdk": "2.1029.2",
    "aws-lambda": "^1.0.7",
    "jest": "^29.7.0",
    "ts-jest": "^29.2.5",
    "ts-node": "^10.9.2",
    "tsc-alias": "^1.8.16",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.9.2"
  },
  "dependencies": {
    "@aws-sdk/client-s3": "^3.899.0",
    "@aws-sdk/client-sqs": "^3.899.0",
    "@aws-sdk/s3-request-presigner": "^3.899.0",
    "@clerk/backend": "^2.14.0",
    "aws-cdk-lib": "2.214.0",
    "constructs": "^10.0.0",
    "dotenv": "^17.2.2",
    "pg": "^8.11.3",
    "uuid": "^13.0.0",
    "winston": "^3.17.0"
  }
}
