{
  "name": "embedding",
  "version": "1.0.0",
  "description": "Embedding microservice for vector operations",
  "bin": {
    "cdk": "bin/cdk.js"
  },
  "scripts": {
    "dev": "tsx src/main.ts",
    "build": "tsc && tsc-alias",
    "watch": "tsc -w",
    "start": "node dist/main.js",
    "clean": "rm -rf dist",
    "test": "jest",
    "cdk": "cdk",
    "cdk:bootstrap": "cdk bootstrap",
    "cdk:synth": "cdk synth",
    "cdk:deploy": "cdk deploy",
    "cdk:destroy:dev": "export NODE_ENV=development && cdk destroy --all --force",
    "cdk:destroy:prod": "export NODE_ENV=production && cdk destroy --all --force",
    "cdk:deploy:dev": "export NODE_ENV=development && cdk bootstrap && cdk synth && cdk deploy --all --require-approval never --outputs-file outputs.json",
    "cdk:deploy:prod": "export NODE_ENV=production && cdk bootstrap && cdk synth && cdk deploy --all --require-approval never --outputs-file outputs.json"
  },
  "keywords": [
    "embedding",
    "vector",
    "microservice"
  ],
  "author": "",
  "license": "ISC",
  "packageManager": "pnpm@10.15.1",
  "devDependencies": {
    "@types/aws-lambda": "^8.10.152",
    "@types/jest": "^29.5.14",
    "@types/node": "^24.5.1",
    "aws-cdk": "2.1029.2",
    "aws-lambda": "^1.0.7",
    "jest": "^29.7.0",
    "ts-jest": "^29.2.5",
    "ts-node": "^10.9.2",
    "tsc-alias": "^1.8.16",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.9.2"
  },
  "dependencies": {
    "@clerk/backend": "^2.14.0",
    "@types/pg": "^8.15.5",
    "aws-cdk-lib": "2.214.0",
    "constructs": "^10.0.0",
    "dotenv": "^17.2.2",
    "openai": "^6.7.0",
    "pg": "^8.16.3",
    "winston": "^3.17.0"
  }
}
